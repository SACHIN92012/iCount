<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.icount">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/scan"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme2">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.gms.DEPENDENCIES"
            android:value="barcode" />

        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScanBarcodeActivity"
            android:label="Scan" />
        <activity
            android:name=".NameListActivity"
            android:label="Entry List" />
        <activity
            android:name=".AddActivity"
            android:label="Add QR code" />
        <activity android:name=".GraphActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".SearchListActivity" />
        <activity
            android:name=".MainActivity2"
            android:theme="@style/AppTheme" />
        <activity android:name=".BlockActivity" />
        <activity android:name=".ScanQRActivity"></activity>
    </application>

</manifest>